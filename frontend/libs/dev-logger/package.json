{
  "name": "@kuumu/dev-logger",
  "version": "1.0.0",
  "description": "Development logging library with Vite middleware for debugging and monitoring",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "development": "./src/vite/index.ts",
      "default": "./dist/vite/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  }
}
